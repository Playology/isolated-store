{
  "name": "@ngxtend/isolated-store",
  "version": "1.0.1",
  "description": "A set of schematics for using Angular and NgRx applying the Isolated Store pattern described at https://github.com/ngxtend/isolated-store",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "predeploy": "npm run build",
    "deploy": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxtend/isolated-store.git"
  },
  "keywords": [
    "schematics",
    "angular",
    "ngrx",
    "strongly typed",
    "isolated store"
  ],
  "author": "Mike Hanson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngxtend/isolated-store/issues"
  },
  "homepage": "https://github.com/ngxtend/isolated-store#readme",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^11.2.1",
    "@angular-devkit/schematics": "^11.2.1",
    "@schematics/angular": "^11.2.1",
    "@types/jasmine": "^3.6.4",
    "@types/node": "^14.14.31",
    "jasmine": "^3.6.4",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.1100.1"
  }
}
